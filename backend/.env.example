# Development Environment Variables
# Copy this to .env and fill in your values

# Database
# For local development, you can use either:
# 1. Local PostgreSQL: postgresql://user:password@localhost:5432/relevia
# 2. Supabase (same as production): postgresql://[user]:[password]@[host]:6543/postgres
DATABASE_URL=postgresql://localhost:5432/relevia

# Security
SECRET_KEY=your-secret-key-here-change-in-production

# API Keys (optional for local dev)
GEMINI_API_KEY=
PINECONE_API_KEY=

# Other settings
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30